{
    "name": "ricardoboss/laravel-roles-migrations",
    "type": "library",
    "description": "Write migrations for your laravel roles.",
    "keywords": [
        "laravel",
        "roles",
        "permissions",
        "migrations"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "laravel/framework": "6.*|7.*",
        "jeremykenedy/laravel-roles": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ricardoboss\\Database\\Migrations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ricardoboss\\Database\\RolesMigrationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
